			"hopium/common/utils/id_idx.sol": {
				"content": "// SPDX-License-Identifier: MIT\npragma solidity 0.8.30;\n\nabstract contract IdIdxUtils {\n\n    function _idxToId(uint256 _idx) internal pure returns (uint256) {\n        return _idx + 1;\n    }\n\n    function _idToIdx(uint256 _id) internal pure returns (uint256) {\n        return _id - 1;\n    }\n}"
			}
