				"getTokenMarketCapUsd(address)": {
					"notice": "Returns market cap in USD for TOKEN (scaled 1e18)."
				},
				"getTokenMarketCapWeth(address)": {
					"notice": "Returns market cap in WETH for TOKEN (scaled 1e18): totalSupply * (WETH per 1 TOKEN). If TOKEN is WETH, this is simply WETH totalSupply (scaled to 1e18)."
				},
				"getTokenUsdPrice(address)": {
					"notice": "Returns USD price of 1 TOKEN (scaled 1e18)"
				},
				"getTokenWethPrice(address)": {
					"notice": "Returns WETH per 1 QUOTE (scaled 1e18) for an arbitrary WETH-QUOTE pair. If QUOTE is WETH, returns 1e18."
				},
				"getWethUsdPrice()": {
					"notice": "Returns USD per 1 WETH (scaled 1e18) using the stored WETH/USD pair."
